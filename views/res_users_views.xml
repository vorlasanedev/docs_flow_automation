<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_users_form_docs_automation" model="ir.ui.view">
        <field name="name">res.users.form.docs.automation</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
        <xpath expr="//page[@name='access_rights']/group" position="inside">
            <group string="Document Automation">
                <field name="docs_position"/>
            </group>
        </xpath>
        </field>
    </record>
</odoo>
